{
    "name": "code-names-api",
    "version": "1.0.0",
    "private": true,
    "bin": {
        "run_code_names": "bin/www"
    },
    "scripts": {
        "build": "tsc --resolveJsonModule",
        "buildEnv": "tsc --resolveJsonModule ./environment.ts",
        "postbuild": "copyfiles ./bin/** ./app/**/*.json ./app/common/emailTemplates/**/*.hbs package.json package-lock.json typings.d.ts tsconfig.json tsconfig.spec.json ./build",
        "start": "npm run start:dev",
        "start:local": "node -r esm ./build/bin/www",
        "start:dev": "node -r esm -r ./ts-node.config.ts ./bin/www",
        "start:int": "node --inspect ./bin/www",
        "testing": "node ./scripts/testing",
        "test": "ts-node node_modules/jasmine/bin/jasmine --config=./jasmine.json",
        "test-mocha": "tsc --resolveJsonModule && mocha",
        "test-mocha-watch": "tsc --resolveJsonModule && mocha -w",
        "test-jest": "mocha -w -r ts-node/register tests/**/*.spec.ts",
        "tscompile": "npm run build"
    },
    "dependencies": {
